{
  "name": "breithbarbot/cropper",
  "type": "symfony-bundle",
  "description": "Simple cropping tool for symfony",
  "keywords": ["crop", "cropping", "image", "avatar", "upload", "symfony-bundle", "form-types", "symfony", "php"],
  "homepage": "https://github.com/breithbarbot/cropper",
  "license": "MIT",
  "authors": [
    {
      "name": "Breith Barbot",
      "email": "b.breith@gmail.com",
      "homepage": "https://breithbarbot.name"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "symfony/config": "^3.4||^4.0",
    "symfony/dependency-injection": "^3.4||^4.0",
    "symfony/form": "^3.4||^4.0",
    "symfony/http-kernel": "^3.4||^4.0",
    "twig/twig": "~1.34||~2.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.10",
    "symfony/phpunit-bridge": "^3.4||^4.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": { "Breithbarbot\\CropperBundle\\": "" }
  },
  "autoload-dev": {
    "psr-4": {
      "Breithbarbot\\CropperBundle\\": "Tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "support": {
    "docs": "https://github.com/breithbarbot/cropper/blob/master/Resources/doc/index.md"
  }
}
