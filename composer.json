      "homepage": "https://breithbarbot.name"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "symfony/config": "^3.0",
    "symfony/dependency-injection": "^3.0",
    "symfony/form": "^3.0",
    "symfony/http-kernel": "^3.0",
    "symfony/http-foundation": "^3.0",
    "twig/twig": "~1.28|~2.0"
