{
  "name": "breithbarbot/cropper",
  "type": "symfony-bundle",
  "description": "Simple cropping tool for symfony 3.4 and later",
  "keywords": ["crop", "cropping", "image", "avatar", "upload", "symfony-bundle", "form-types", "symfony3", "php"],
  "homepage": "https://gitlab.com/breithbarbot/cropper",
  "license": "MIT",
  "authors": [
    {
      "name": "Breith Barbot",
      "email": "b.breith@gmail.com",
      "homepage": "https://breithbarbot.name"
    }
  ],
  "require": {
    "php": ">=7.0",
    "sensiolabs/security-checker": "^4.1",
    "stof/doctrine-extensions-bundle": "^1.3",
    "symfony/asset": "^3.4",
    "symfony/config": "^3.4",
    "symfony/dependency-injection": "^3.4",
    "symfony/form": "^3.4",
    "symfony/http-foundation": "^3.4",
    "symfony/http-kernel": "^3.4",
    "twig/twig": "~1.34|~2.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.10",
    "symfony/phpunit-bridge": "^3.4"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": { "Breithbarbot\\CropperBundle\\": "" }
  },
  "autoload-dev": {
    "psr-4": {
      "Breithbarbot\\CropperBundle\\": "Tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "security-checker security:check": "script"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  },
  "support": {
    "docs": "https://gitlab.com/breithbarbot/cropper/blob/master/Resources/doc/index.md"
  }
}
