{
    "name": "breithbarbot/cropper",
    "description": "Simple cropping tool for symfony",
    "version": "3.0.0",
    "type": "symfony-bundle",
    "homepage": "https://github.com/breithbarbot/cropper",
    "readme": "README.md",
    "license": "MIT",
    "authors": [{"name": "Breith Barbot", "email": "b.breith@gmail.com", "homepage": "https://breithbarbot.name"}],
    "support": {"docs": "https://github.com/breithbarbot/cropper/blob/master/Resources/doc/index.md"},
    "require": {
        "php": "^7.4",
        "ext-xml": "*",
        "symfony/config": "4.4.*",
        "symfony/dependency-injection": "4.4.*",
        "symfony/form": "4.4.*",
        "symfony/http-kernel": "4.4.*",
        "symfony/routing": "4.4.*",
        "symfony/twig-bundle": "4.4.*"
    },
    "require-dev": {
        "symfony/test-pack": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "autoload": {
        "psr-4": {
            "Breithbarbot\\CropperBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Breithbarbot\\CropperBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    }
}
